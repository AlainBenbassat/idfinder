<?xml version="1.0"?>
<extension key="idfinder" type="module">
  <file>idfinder</file>
  <name>ID Finder</name>
  <description>Finds the contact id of contacts in an Excel file</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>Alain Benbassat</name>
      <email>alain@businessandcode.eu</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Main Extension Page">https://FIXME</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-12-10</releaseDate>
  <version>1.0.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>6.4</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Idfinder</namespace>
    <format>25.01.1</format>
    <angularModule>crmIdfinder</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
